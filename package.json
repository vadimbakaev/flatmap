  "private": true,
  "dependencies": {
    "antd": "^4.4.1",
    "babel-plugin-import": "^1.13.0",
    "customize-cra": "^1.0.0",
    "less": "^3.11.3",
    "less-loader": "^6.2.0",
    "react": "^16.13.1",
    "react-app-rewired": "^2.1.6",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "test-coverage": "react-app-rewired test --coverage && cat ./coverage/lcov.info"
