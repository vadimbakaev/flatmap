{
  "name": "flatmap",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^4.10.2",
    "babel-plugin-import": "^1.13.3",
    "customize-cra": "^1.0.0",
    "less": "^3.12.2",
    "less-loader": "^7.2.1",
    "react": "^17.0.1",
    "react-app-rewired": "^2.1.6",
    "react-dom": "^17.0.1",
    "react-scripts": "^4.0.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "test-coverage": "react-app-rewired test --coverage && cat ./coverage/lcov.info"
