{
  "name": "flatmap",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^4.8.0",
    "babel-plugin-import": "^1.13.1",
    "customize-cra": "^1.0.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.2",
    "react": "^16.14.0",
    "react-app-rewired": "^2.1.6",
    "react-dom": "^16.14.0",
    "react-scripts": "^3.4.3"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "test-coverage": "react-app-rewired test --coverage && cat ./coverage/lcov.info"
